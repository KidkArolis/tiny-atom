{
  "name": "tiny-atom",
  "version": "3.0.0-beta.0",
  "description": "Minimal, yet awesome, state management.",
  "main": "index.js",
  "scripts": {
    "coverage": "nyc --reporter html ava",
    "test": "standard && nyc ava",
    "version": "node ./tasks/build.js",
    "release": "np --contents dist",
    "release-beta": "np --tag=beta --contents=dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/QubitProducts/tiny-atom.git"
  },
  "contributors": [
    "Karolis Narkevicius <karolis@qubit.com>",
    "Alan Clarke <alan@qubit.com>"
  ],
  "license": "ISC",
  "devDependencies": {
    "ava": "^0.25.0",
    "buble": "^0.19.3",
    "execa": "^0.10.0",
    "jetpack": "^0.8.0",
    "jsdom": "^11.3.0",
    "np": "^3.0.4",
    "nyc": "^12.0.2",
    "preact": "^8.2.9",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "standard": "^11.0.0"
  },
  "standard": {
    "ignore": [
      "docs/public"
    ]
  },
  "ava": {
    "files": "test/test-*.js",
    "require": "./test/helpers/setup.js"
  },
  "dependencies": {
    "deep-diff": "^1.0.1"
  }
}
