{
  "name": "tiny-atom",
  "version": "5.0.0-pre.2",
  "description": "Pragmatic and concise state management.",
  "main": "tiny-atom.cjs.js",
  "module": "tiny-atom.esm.js",
  "sideEffects": false,
  "scripts": {
    "coverage": "nyc --reporter html ava",
    "test": "npm run lint && nyc ava",
    "lint": "healthier && prettier --check '**/*.{js,json,css,yml}'",
    "format": "prettier --write '**/*.{js,json,css,yml}'",
    "build": "node ./tasks/build.js",
    "build:docs": "sitedown . -b docs-next --layout layout.html",
    "version": "npm run build",
    "release": "np --contents dist",
    "release-beta": "np --tag=beta --contents=dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KidkArolis/tiny-atom.git"
  },
  "contributors": [
    "Karolis Narkevicius <hello@kn8.lt>",
    "Alan Clarke <alan@qubit.com>"
  ],
  "license": "ISC",
  "dependencies": {
    "deep-diff": "^1.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.5",
    "@babel/plugin-transform-react-jsx": "^7.19.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/register": "^7.18.9",
    "ava": "^5.1.0",
    "execa": "^6.1.0",
    "healthier": "^6.2.0",
    "jetpack": "^0.22.0",
    "jsdom": "^20.0.3",
    "np": "^7.6.2",
    "nyc": "^15.1.0",
    "preact": "^10.11.3",
    "prettier": "^2.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "ava": {
    "files": [
      "test/test-*.js"
    ],
    "require": [
      "@babel/register",
      "./test/helpers/setup.js"
    ]
  },
  "np": {
    "releaseDraft": false
  }
}
